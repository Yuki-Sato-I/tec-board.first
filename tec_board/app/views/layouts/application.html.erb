<!DOCTYPE html>
<html>
  <head>
    <title>TecBoard</title>
    <%= render 'layouts/first_config' %>
    <%= render 'layouts/shim' %>
  </head>

  <body>
    <%= render 'layouts/header' %>
    <% if flash[:sucess] %>
      <div class="flash">
        <h1><%= flash[:sucess] %></h1>
      </div>
    <% end %>
    <%= yield %>
    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>
